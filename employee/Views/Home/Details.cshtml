@model employee.ViewModels.HomeDetailsViewModel

<!DOCTYPE html>
<html>
<head>
  <title>@Model.title</title>
</head>
<body>
  <h2> Details for Employee @Model.Employee.Name</h2>
  <p>ID: @Model.Employee.ID</p>
